{
    "subHeader":{
        "pageHeader":{
            "text":"プロフィール"
        }
    },
    "memberInfo":{
        "class" : "memberInfo cf",
        "userInf":{
            "class":"userInf outsideArea cf"
        }
    },
    "nameSummary":{
        "text":"名前"
    },
    "nameTextbox":{
        "textboxArea":{
            "type":"text",
            "name":"user_name"
        }
    },
    "nameKanaSummary":{
        "text":"カナ"
    },
    "nameKana":{
        "textboxArea":{
            "type":"text",
            "name":"name_kana"
        }
    },
    "userSexSummary":{
        "text":"性別"
    },
    "userSex":{
        "select":{
            "name":"user_sex",
            "selectOption":{
                "text":"女性",
                "value":"2"
            },
            "selectOption2":{
                "text":"男性",
                "value":"1"
            }
        }
    },
    "birthDayDateSummary":{
        "text":"生年月日"
    },
    "birthDayDate":{
        "textboxArea":{
            "type":"date",
            "name":"birthday_date"
        }
    },
    "zipCodeSummary":{
        "text":"郵便番号"
    },
    "zipCode":{
        "textboxArea":{
            "type":"text",
            "name":"zip_code"
        }
    },
    "addressSummary":{
        "text":"住所"
    },
    "addressArea":{
        "textArea":{
            "name":"address"
        }
    },
    "mailAddressSummary":{
        "text":"メールアドレス"
    },
    "mailAddress":{
        "textboxArea":{
            "type":"email",
            "name":"mail_address"
        }
    },
    "telephoneSummary":{
        "text":"電話番号"
    },
    "telephoneTextbox":{
        "textboxArea":{
            "type":"text",
            "name":"telephone"
        }
    },
    "telephone2Summary":{
        "text":"緊急番号"
    },
    "telephone2Textbox":{
        "textboxArea":{
            "type":"text",
            "name":"telephone2"
        }
    },
    "mailMagaSettingSummary":{
        "class":"inputLabel mailMaga",
        "text":"メルマガ受信設定"
    },
    "mailMagaSetting":{
        "acceptLabel":{
            "acceptRadio":{
                "type":"radio",
                "name":"mail_deny",
                "value":"0"
            },
            "acceptLabelText":{
                "text":"受信する"
            }
        },
        "denyLabel":{
            "denyRadio":{
                "type":"radio",
                "name":"mail_deny",
                "value":"2"
            },
            "denyLabelText":{
                "text":"受信しない"
            }
        }
    },
    "updateButton":{
        "class":"updateButton fRight",
        "type":"submit",
        "name":"button",
        "value":"更新"
    },
    "passLinkArea":{
        "class":"passLinkArea textCenter fRight",
        "passLink":{
            "href":"#window/member/page/memberPassword.html",
            "text":"パスワード変更"
        }
    },
    "updateUserInf":{
        "db_setQuery":"CALL updateUserProfile('user_name', 'name_kana', 'zip_code', 'address', 'user_sex', 'birthday_date', 'telephone','telephone2' ,'mail_address', 'mail_deny', 'userId');"
    },
    "getUserInf":{
        "db_getQuery":"CALL getUserProfile(@result, 'userId'); SELECT @result AS 'result';",
        "userId":{
            "value":""
        },
        "memberInfoValue":{
            "user_name":{
                "text":""
             },
             "name_kana":{
                "text":""
            },
            "user_sex":{
                "text":""
             },
             "birthday_date":{
                "text":""
            },
            "zip_code":{
                "text":""
            },
            "address":{
                "text":""
            },
            "mail_address":{
                "text":""
            },
            "telephone":{
                "text":""
            },
            "telephone2":{
                "text":""
            },
            "mail_deny":{
                "text":""
            }
        }
    }
}